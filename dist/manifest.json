{
  "name": "pain",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["./background.bundle.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./content.bundle.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
