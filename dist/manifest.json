{
  "name": "pain",
  "version": "1.0",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "background": {
    "scripts": ["./build/Background.bs.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./build/Content.bs.js"]
    }
  ],
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
